= render :partial => "layouts/navbar"

= render :partial => "layouts/search"

= render :partial => "layouts/tv-count"

%div.bradflix-movies
  %div.container

    %div.row
      %div.col-md-2.hidden-xs.hidden-sm
        = render :partial => "layouts/sidebar"

      %div.col-md-10
        %ul
          -@all_tv_shows.each do |show|
            %li
              %a{:href => "#{show[1][:href]}"}
                = "#{show[1][:link]}"